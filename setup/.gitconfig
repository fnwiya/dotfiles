[core]
	excludesfile = /Users/fnwiya/.gitignore_global
    editor = vim
[include]
    path = ~/.gitconfig.local
[color]
    ui          = auto
    status      = auto
    diff        = auto
    branch      = auto
    interactive = auto
    grep        = auto
[user]
    name  = fnwiya
    email = fnwiya@gmail.com
[push]
    default = simple
[ghq]
    root = ~/sandbox
    root = ~/go/src
[alias]
    graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative